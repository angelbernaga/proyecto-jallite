<?xml version='1.0' encoding='utf-8'?>
<widget id="mx.com.asesorescorporativos.leyesjalisco" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Leyes Estatales: Jalisco</name>
    <description>
        40 Leyes del Estado de Jalisco y otras herramientas para abogados.
    </description>
    <author email="angelbernaga@gmail.com" href="http://asesorescorporativos.com.mx">
        Angel Bernaga
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
